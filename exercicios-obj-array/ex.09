// Objeto representando a conta
const conta = {
    agencia: 1234,
    numero: 56789,
    senha: 4321,
    saldo: 1000,
    historico: []
  };
  
  // Função para validar acesso
  function validarAcesso(agencia, numero, senha) {
    if (
      agencia === conta.agencia &&
      numero === conta.numero &&
      senha === conta.senha
    ) {
      return true;
    } else {
      return false;
    }
  }
  
